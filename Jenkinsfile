pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                echo 'Build the application'
                sh 'mvn --version'
            }
        stage('test') {
            steps {
                echo 'Test the application'
                sh 'mvn --version'
            }
         stage('deploy') {
            steps {
                echo 'Deploy the application'
                sh 'mvn --version'
            }
    }
}
